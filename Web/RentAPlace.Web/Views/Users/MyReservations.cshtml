@model RentAPlace.Web.ViewModels.Users.ReservationsListViewModel;
@{
    ViewData["Title"] = "My Reservations";
}
<h1>@ViewData["Title"], page: @Model.PageNumber</h1>

<table class="table table-hover">
    <tbody>
    <thead>
    <tr>
        <th scope="col">Real Estate Type Name</th>
        <th scope="col">District Name</th>
        <th>From</th>
        <th scope="col">To</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var booking in Model.Reservations)
    {
        <tr>
            <td>@booking.RealEstateTypeName</td>
            <td>@booking.DistrictName</td>
            <td>@booking.From.ToString("dd / MM / yyyy")</td>
            <td>@booking.To.ToString("dd / MM / yyyy")</td>
        </tr>
    }
    </tbody>
</table>

<partial name="_PagingPartial" model="@Model" />